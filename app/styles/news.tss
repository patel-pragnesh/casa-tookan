	borderRadius: 4,
	borderWidth: 1,
	backgroundColor: "#ffffff",
	borderColor: "#ffffff"
}


".empty": {
	width: Ti.UI.FILL,
	height: 60,
	color: "#ececec",
	backgroundColor: "transparent"
}
